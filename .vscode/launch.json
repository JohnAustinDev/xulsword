{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "server",
      "runtimeExecutable": "bash",
      "runtimeArgs": ["${workspaceFolder}/src/server/start-server.sh"],
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "App electron server",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["start:main"],
      "skipFiles": ["<node_internals>/**"],
      "outFiles": [
        "${workspaceFolder}/build/app/dist/**/*",
        "${workspaceFolder}/node_modules/electron-devtools-installer/dist/utils.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "WebApp node server",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["start:server"],
      "skipFiles": ["<node_internals>/**"],
      "outFiles": ["${workspaceFolder}/build/webapp/dist/**/*"]
    },
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}"
    }
  ]
}
